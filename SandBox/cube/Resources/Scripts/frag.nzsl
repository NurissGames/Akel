[nzsl_version("1.0")]
module;

import * from Akel.VertexOutput;
import * from Akel.FragmentOutput;

external
{
	[set(1), binding(1)] texSampler: sampler2D[f32]
}

[entry(frag)]
fn main(input: VertOut) -> FragOut
{
	let output: FragOut;
	output.color = input.color * texSampler.Sample(input.uv);
	return output;
}
